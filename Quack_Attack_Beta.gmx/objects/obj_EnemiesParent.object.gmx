            <kind>1</kind>
            <string>image_angle = direction;

// check obj_TomahawkUpgradeProjectile, these 2 lines allow to take damage only once for every axe
if (!place_meeting(x, y, obj_TomahawkUpgradeProjectile)) canBeDamagedByAxe = true;
else if (place_meeting(x, y, obj_TomahawkUpgradeProjectile)) canBeDamagedByAxe = false;

if (poisoned == 1){
    currentHp -= (maxHp/800);
    poisonedCountdown -= 1;
    if (poisonedCountdown == 0){
        poisoned = 0;
    }
}

if (slowed == 1){
    image_blend = c_silver;
    path_speed = pathSlowSpeed;
    slowedCountdown -= 1;
    if (slowedCountdown == 0){
        path_speed = pathSpeed;
        slowed = 0;
        image_blend = c_white;
    }
}

if (currentHp &lt;= 0){
    obj_Money.currentMoney += moneyValue;
    instance_destroy();
}

if (place_meeting(x, y, obj_Web) &amp;&amp; flyingEnemy == 0){
    path_speed = pathSlowSpeed;
}
else if (!place_meeting(x, y, obj_Web) &amp;&amp; slowed == 0 &amp;&amp; flyingEnemy == 0){
    path_speed = pathSpeed;
}
</string>
